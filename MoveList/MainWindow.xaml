        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:MoveList"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="835" Initialized="Window_Initialized">
    <Window.Resources>
        <JournalEntryListConverter x:Key="JournalEntryListConverter"/>
    </Window.Resources>
    <Grid HorizontalAlignment="Left" Height="398" VerticalAlignment="Top" Width="787">
        <MediaElement x:Name="Media" HorizontalAlignment="Left" Height="354" Margin="123,16,-613,-270" VerticalAlignment="Top" Width="590"/>
        <Button x:Name="button" Content="Button" HorizontalAlignment="Left" VerticalAlignment="Top" Width="75" Click="button_Click"/>
    </Grid>

</Window>
